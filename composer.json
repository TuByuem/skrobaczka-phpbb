{
    "name": "tubyuem/skrobaczka-phpbb",
    "authors": [
        {
            "name": "TuByuem",
            "email": "tubyuem@wp.pl"
        }
    ],
    "require": {
        "symfony/console": "~2.6",
        "symfony/dependency-injection": "~2.6",
        "fabpot/goutte": "~2.0",
        "symfony/yaml": "~2.6",
        "symfony/config": "~2.6",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "autoload": {
        "files": ["TuByuem/Skrobaczka/DependencyInjection/AddConsoleCommandPass.php"],
        "psr-0": {
            "": "."
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    }
}
